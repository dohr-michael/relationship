{
  "name": "relationship-app",
  "version": "1.0.0",
  "description": "Portal to manage relationship between differents kind of entities",
  "config": {
    "devHost": "localhost",
    "devPort": 8001
  },
  "scripts": {
    "build": "NODE_ENV=production tsc && NODE_ENV=production webpack",
    "build:dev": "NODE_ENV=development tsc",
    "start": "webpack-dev-server",
    "cover": "NODE_ENV=test nyc npm test",
    "test": "npm run build:dev && NODE_ENV=test mocha --recursive -r test/jsdom.js -r test/css-loader.js -r ts-node/register test/**/*-test.ts test/**/*-test.tsx"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "main": "index.js",
  "author": "Michael DOHR",
  "license": "GNU",
  "dependencies": {
    "@types/classnames": "^2.2.0",
